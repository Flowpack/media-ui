{
  "browsers": {
    "path": "firefox",
    "cmd": "--window-size=1280,720"
  },
  "src": ["packages/media-module/tests/**/*.ts"],
  "appCommand": "yarn dev:test",
  "appInitDelay": 2000,
  "screenshots": {
    "takeOnFails": true
  },
  "debugOnFail": false,
  "stopOnFirstFail": true,
  "hostname": "127.0.0.1",
  "baseUrl": "http://127.0.0.1:8000",
  "retryTestPages": true,
  "pageLoadTimeout": 10000,
  "pageRequestTimeout": 60000,
  "skipJsErrors": true
}
