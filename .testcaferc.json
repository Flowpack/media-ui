{
  "browsers": {
    "path": "chromium",
    "cmd": "--window-size=1280,720"
  },
  "src": ["packages/media-module/tests/**/*.ts"],
  "appCommand": "yarn ts-node --project packages/dev-server/tsconfig.json packages/dev-server/src/server.ts",
  "screenshots": {
    "takeOnFails": true
  },
  "stopOnFirstFail": true,
  "hostname": "localhost",
  "retryTestPages": true,
  "pageLoadTimeout": 10000,
  "pageRequestTimeout": 60000,
  "skipJsErrors": {
    "stack": "/.*chrome-extension.*/ig"
  }
}
