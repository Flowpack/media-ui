{
  "name": "neos-media-ui",
  "version": "1.0.0",
  "license": "GNU GPLv3",
  "private": true,
  "scripts": {
    "lint": "tsc --noEmit && eslint 'Resources/Private/JavaScript/**/*.{js,jsx,ts,tsx}'",
    "test": "exit 0",
    "dev": "node Resources/Private/JavaScript/dev/server",
    "watch:parcel": "parcel watch Resources/Private/JavaScript/src/index.tsx --out-dir Resources/Public/Assets -o main.bundle.js",
    "watch:neos-wrap": "node-sass extra-neos-wrapper.scss -o Resources/Public/Assets/ && node-sass -w extra-neos-wrapper.scss -o Resources/Public/Assets/",
    "watch": "concurrently -k npm:watch:parcel npm:watch:neos-wrap",
    "build": "parcel build Resources/Private/JavaScript/src/index.tsx --out-dir Resources/Public/Assets -o main.bundle.js && node-sass extra-neos-wrapper.scss -o Resources/Public/Assets/"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-regular-svg-icons": "^5.8.0",
    "@fortawesome/free-solid-svg-icons": "^5.8.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@friendsofreactjs/react-css-themr": "^3.3.2",
    "@neos-project/react-ui-components": "^5.1.4",
    "apollo-boost": "^0.4.7",
    "classnames": "^2.2.6",
    "graphql": "^14.6.0",
    "lodash.omit": "^4.3.0",
    "lodash.throttle": "^4.0.8",
    "raf": "^3.4.1",
    "react": "^16.12.0",
    "react-dnd": "~7.0.2",
    "react-dnd-html5-backend": "~7.0.2",
    "react-dom": "^16.12.0",
    "react-jss": "^10.0.4",
    "react-modal": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "express": "^4.17.1",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.4",
    "postcss-custom-properties": "^9.1.1",
    "postcss-modules": "^1.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.8.2"
  },
  "browserslist": [
    "> 1%",
    "Last 2 versions"
  ]
}
