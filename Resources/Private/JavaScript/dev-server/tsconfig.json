{
  "extends": "../../../../tsconfig.json",
  "include": [
    "src/**/*.ts",
    "node_modules/@media-ui/feature-asset-usage/src/**/*",
    "node_modules/@media-ui/feature-asset-variants/src/**/*",
    "node_modules/@media-ui/feature-concurrent-editing/src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "rootDir": "src",
    "jsx": "react",
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "types": ["node"]
  },
  "ts-node": {
    "esm": true
  }
}
